          <stringProp name="BeanShellAssertion.query">import org.apache.jmeter.assertions; 
import org.apache.jmeter.samplers.SampleResult; 
import org.apache.jmeter.assertions.AssertionResult; 
import org.json.JSONObject;
String response_data = prev.getResponseDataAsString();
//JSONObject data_obj = new JSONObject(response_data);
//log.info(response_data);
String data_type=&quot;&quot;;
String url=&quot;&quot;;
//注意：如果您正在使用(导入)org.json.simple.JSONArray,则必须使用JSONArray.size()来获取所需的数据.
//但是,如果您使用的是org.json.JSONArray,请使用JSONArray.length().

try{
	JSONObject data_obj = new JSONObject(response_data);
	log.info(response_data);
	data_type = data_obj.get(&quot;data&quot;).get(&quot;data_info&quot;).get(&quot;data_type&quot;);
	url =data_obj.get(&quot;data&quot;).get(&quot;data_info&quot;).get(&quot;url&quot;);
	log.info(&quot;data_type：&quot;+data_type);
}
catch(Exception e){
	Failure = true;
    	log.info(&quot;数据解析异常，断言失败&quot;);
    	FailureMessage = &quot;数据解析异常，断言失败&quot;;
//    	throw e;
//	prev.setStopThread(true);
	break;
	//如果数据解析异常，直接停止所有程序，慎用 
//	System.exit(1);
}


if(data_type !=&quot;&quot;)
{
	if(data_type.equals(&quot;2&quot;)){
		//String playauth = data_obj.get(&quot;data&quot;).get(&quot;data_info&quot;).get(&quot;playauth&quot;);
		if(url.equals(&quot;&quot;)){
			//String playauth = data_obj.get(&quot;data&quot;).get(&quot;data_info&quot;).get(&quot;playauth&quot;);
			Failure = true;
    			log.info(&quot;视频url为空，断言失败&quot;);
    			FailureMessage = &quot;视频url为空，断言失败&quot;;
		}
		else{
			Failure = false;
    			log.info(&quot;视频正常,断言通过&quot;);
    			FailureMessage = &quot;视频正常,断言通过&quot;;
		}
	}

    else{
    		if(url.equals(&quot;&quot;)){
     		Failure = true;
		    	log.info(&quot;url为空，断言失败&quot;);
		    	FailureMessage = &quot;url为空，断言失败&quot;;
    		}else if(url.length()&gt;100){
    			Failure = false;
    			log.info(&quot;文件url太长，断言失败&quot;);
    			FailureMessage = &quot;文件url太长，断言失败&quot;;
    		}else{
    			Failure = false;
    			log.info(&quot;文件url不为空，断言通过&quot;);
    			FailureMessage = &quot;文件url不为空，断言通过&quot;;
    		}
    	}
}
else{
	Failure = true;
    	log.info(&quot;data_type为空，断言失败&quot;);
    	FailureMessage = &quot;data_type为空，断言失败&quot;;
    	//断言失败则停止线程执行
	//prev.setStopThread(true);
	}</stringProp>
